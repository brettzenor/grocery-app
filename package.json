{
  "name": "litstack-seed",
  "version": "0.3.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "test": "nyc mocha"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@litstack/core": "^0.5.3",
    "rxjs": "^6.0.0-beta.4",
