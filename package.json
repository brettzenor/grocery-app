    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nyc": "^11.7.3"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
